<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression suit " verbose="1">
    <groups>
        <run>
            <include name="regression"></include>
        </run>
    </groups>
    <listeners>
        <listener class-name="com.orangehrmlive.demo.costumlistener.CustomListener"></listener>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>
    <test name="regression test">
        <classes>
            <class name="com.orangehrmlive.demo.testsuits.LoginTest"></class>
            <class name="com.orangehrmlive.demo.testsuits.UsersTest"></class>
        </classes>
    </test>
</suite>